<?xml version="1.0" encoding="UTF-8"?>

<phpunit bootstrap="bootstrap.php" backupGlobals="false" colors="true">
    <testsuites>
        <testsuite name="Doctrine Template Positionable Test Suite">
            <directory>./</directory>
        </testsuite>
    </testsuites>

    <filter>
        <whitelist>
            <directory>../src/</directory>
            <exclude>
                <directory>./</directory>
            </exclude>
        </whitelist>
    </filter>
</phpunit>
